block variables

doctype html
html(lang="ru")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    meta(name="description" content=obj.description)
    title RTUITLab — #{obj.title}
    link(rel="stylesheet" href="../layout/swiper/swiper-bundle.min.css")
    link(rel="canonical" href="https://rtuitlab.dev")
    link(rel="stylesheet" href="../layout/projectPageTemplate/projectPageTemplate.scss")
    link(rel="stylesheet" href="../indexStyles.scss")
    script(src="../js/projectPageTemplate/projectPageTemplate.js", defer)
    script(async="async").
      let scripts = [
        process.env.MARKDOWN_IT,process.env.SWIPER_LINK
      ]
      for (let i of scripts) {
        var js = document.createElement('script');
        js.src = i;
        var head = document.getElementsByTagName('head')[0];
        head.appendChild(js);
      }

    script(defer).
      window.onload = function() {
        var md = window.markdownit({
          html: true,
          xhtmlOut: true,
          breaks: true,
          linkify: true,
          typographer: true,
          quotes: '<<>>'
        })
        document.getElementById("longdescription").innerHTML = md.render(`#{obj.description}`)

        console.log(md.render(`#{obj.description}`))
      }

    script(type='application/ld+json').
      {
      "@context": "https://schema.org",
      "@type": "BlogPosting",
      "mainEntityOfPage": {
      "@type": "WebPage",
      "@id": "https://rtuitlab.dev"
      },
      "headline": "#{obj.title}",
      "image": "#{obj.images[0]}",
      "author": {
      "@type": "Organization",
      "name": "RTUITLab",
      "url": "https://rtuitlab.dev"
      },
      "publisher": {
      "@type": "Organization",
      "name": "RTUITLab",
      "logo": {
      "@type": "ImageObject",
      "url": ""
      }
      },
      "datePublished": ""
      }

  body.projectPageTemplate
    header
      section.buttonContainer
        button(onclick="window.history.back();")
          img(src="../images/icons/buttonArrow.svg" alt="")
          | Назад

        .titleContainer
          h1 #{obj.title}

      section.cover
        img(src="../"+obj.images[0], alt="")


    main
      section.content
        .galleryParent
          .swiper.projectsSwiperParent
            .swiper-wrapper
              each object in obj.images.splice(1,obj.images.length)
                .swiper-slide
                  img(src="../"+object, alt="" loading='lazy')
          .swiper-pagination.projectsSwiperPagination

        section.info
          article
            p.tags
              each object in obj.tags.slice(0,4)
                if (obj.tags.indexOf(object)==obj.tags.slice(0,4).length-1)
                  | #{object}
                else
                  | #{object+" | "}
            section#longdescription
            .infoBlock
              h3 Технологии
              section.itemsParent
                each object in obj.tech
                  .item
                    p #{object}

            .infoBlock
              h3 Разработчики
              section.itemsParent
                each object in obj.developers
                  .item
                    p #{object}

            .infoBlock
              h3 Исходный код
              section.itemsParent
                each object in obj.sourcecode
                  .item
                    a(href=object.link target="_blank")
                      =object.name

      section.otherInfo
        block otherInfo